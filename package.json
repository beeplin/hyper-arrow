{
  "name": "hyper-arrow",
  "version": "0.1.1",
  "description": "super tiny front-end UI framework",
  "main": "dist/full.min.js",
  "module": "dist/full.min.js",
  "scripts": {
    "build": "rimraf dist && tsc && uglifyjs src/core.js --compress --mangle --source-map --module --output dist/core.min.js && uglifyjs src/full.js --compress --mangle --source-map --module --output dist/full.min.js",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beeplin/hyper-arrow.git"
  },
  "keywords": [
    "front-end",
    "hyperscript",
    "reactivity",
    "vue",
    "signal"
  ],
  "author": "Beep LIN",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beeplin/hyper-arrow/issues"
  },
  "homepage": "https://github.com/beeplin/hyper-arrow#readme",
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "uglify-js": "^3.19.2"
  }
}
